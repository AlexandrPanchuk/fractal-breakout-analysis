{
  "name": "fractal-detector",
  "version": "1.0.0",
  "description": "Daily fractal detection for forex pairs",
  "main": "fractal-detector.ts",
  "scripts": {
    "start": "ts-node fractal-detector.ts",
    "monitor": "ts-node fractal-monitor.ts",
    "stats": "ts-node -e \"import('./trading-stats').then(m => m.generateTradingContext())\"",
    "web": "ts-node web-server.ts",
    "scrape-forexfactory": "ts-node forexfactory-scraper.ts",
    "detect-fractals": "ts-node improved-fractal-detector.ts",
